{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "scroll-behavior"
        ]
      }
    ],
    "detect-css-reflows/composite": "off",
    "detect-css-reflows/layout": "off",
    "detect-css-reflows/paint": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[allow]"
        ]
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "frame-title": "off"
      }
    ]
  }
}